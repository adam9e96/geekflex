{
  "name": "geekflex-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 5037 --host",
    "release": "vite --port 8070 --host",
    "build": "vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,css,json}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,css,json}\"",
    "preview": "vite preview",
    "update:check": "ncu",
    "update:all": "ncu -u && npm install",
    "update:interactive": "ncu -i"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^7.1.0",
    "@headlessui/react": "^2.2.9",
    "@tailwindcss/vite": "^4.1.18",
    "axios": "^1.13.2",
    "pretendard": "^1.3.9",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.11.0",
    "react-toastify": "^11.0.5",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "autoprefixer": "^10.4.23",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.26",
    "globals": "^16.5.0",
    "postcss": "^8.5.6",
    "prettier": "^3.7.4",
    "tailwindcss": "^4.1.18",
    "vite": "^7.3.0"
  }
}
